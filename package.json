{
	"name": "website",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"watch:sass": "sass  --watch src/sass:public/css",
		"watch:eleventy": "eleventy --serve --port=8081",
		"build:sass": "sass  --no-source-map src/sass:public/css",
		"build:eleventy": "eleventy",
		"postbuild": "postcss public/css/*.css -u autoprefixer cssnano -r",
		"build": "npm-run-all build:sass build:eleventy",
		"start": "npm-run-all build:sass --parallel watch:*"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"type": "module",
	"dependencies": {
		"@11ty/eleventy": "^3.1.2",
		"autoprefixer": "^10.4.21",
		"cssnano": "^7.1.0",
		"js-yaml": "^4.1.0",
		"markdown-it": "^14.1.0",
		"npm-run-all": "^4.1.5",
		"nunjucks": "^3.2.4",
		"postcss": "^8.5.6",
		"postcss-cli": "^11.0.1",
		"sass": "^1.89.2"
	}
}
